{
  "expo": {
    "name": "cluey-mobile",
    "slug": "cluey-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Icon-1024.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#005046"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "com.clueyconsumer.clueymobile",
      "googleServicesFile": "./assets/google-services.json"
    },
    "ios": {
      "bundleIdentifier": "com.clueyconsumer.clueymobile",
      "supportsTablet": true,
      "googleServicesFile": "./assets/GoogleService-Info.plist",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.726028660917-it17vlgku3ska4dm57h87cbequljvno3"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "appVariables": {
        "production": {
          "apiServerUrl": "md-cluey-test.herokuapp.com",
          "httpProtocol": "https",
          "wsProtocol": "wss",
          "categoryImageUrlPrefix": "http://md-cluey-test.herokuapp.com/categories/"
        },
        "heroku-test": {
          "apiServerUrl": "md-cluey-test.herokuapp.com",
          "httpProtocol": "https",
          "wsProtocol": "wss",
          "categoryImageUrlPrefix": "http://md-cluey-test.herokuapp.com/categories/"
        },
        "staging": {
          "apiServerUrl": "md-cluey-staging.herokuapp.com",
          "httpProtocol": "https",
          "wsProtocol": "wss",
          "categoryImageUrlPrefix": "http://md-cluey-test.herokuapp.com/categories/"
        },
        "development": {
          "apiServerUrlProd": "md-cluey-test.herokuapp.com",
          "apiServerUrl": "localhost:4004",
          "httpProtocol": "http",
          "wsProtocol": "ws",
          "categoryImageUrlPrefix": "http://md-cluey-test.herokuapp.com/categories/",
          "brandLogoUrlPrefix": "https://localhost:4004/logos/",
          "categoriesLogoUrlPrefix": "https://localhost:4004/categories/"
        }
      },
      "uplead": {
        "apiKey": "b3ccb6d9ca316379213dff10bfd8227e"
      },
      "bugsnag": {
        "apiKey": "bb37cfd6867c09cb329bbbc56ee6f6a5"
      }
    },
    "hooks": {
      "postPublish": [
        {
          "file": "@bugsnag/expo/hooks/post-publish.js",
          "config": {}
        }
      ]
    }
  }
}
